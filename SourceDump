class Dice:
    def __init__(self, sides, tries):
        self.sides = sides
        self.tries = tries

    def roll(self):
        streak = []
        for ii in range(self.tries):
            streak.append(random.randint(1, self.sides))

        tup = tuple(streak)
        return tup


kauliukas = Dice(16, 4)

print(kauliukas.roll())
